CA;AACA;AACA;AACA","file":"dataDetails.vue","sourceRoot":"src\\views\\account\\components","sourcesContent":["<template>\r\n  <Card style=\"margin: 10px;\">\r\n    <div title=\"数据详情\">\r\n      <div class=\"group-panel-box blue\">\r\n        <div class=\"panel-title\">简单查询</div>\r\n        <div class=\"panel-line\"></div>\r\n      </div>\r\n    </div>\r\n    <div style=\"overflow: hidden;\">\r\n      <div style=\"float: right\">\r\n        <Input v-model=\"value\" placeholder=\"请输入岗位名称\" \r\n\t\t\t\t\tstyle=\"width: 180px; border: rgba(0, 0, 0, 0.5);margin: 10px;\" />\r\n        <Button \r\n\t\t\t\t\ttype=\"success\" @click=\"getOneinfo\" class=\"submit\" \r\n\t\t\t\t\tstyle=\"background: rgba(0, 0, 0, 0.5);border: rgba(0, 0, 0, 0.5); font-weight: 900;\">查询\r\n\t\t\t\t</Button>\r\n      </div>\r\n    </div>\r\n    <Table height=\"520\" stripe border :columns=\"columns5\" :data=\"data5\"></Table>\r\n  </Card>\r\n</template>\r\n<script>\r\nimport Server from \"@/core/server\";\r\nimport { services } from \"@/core/config/services\";\r\nimport houseInfoId from \"@/vuex/store\";\r\nimport dataRap from \"@/vuex/store\";\r\nimport aroundInfo from \"@/vuex/store\";\r\nimport housePoint from \"@/vuex/store\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      form:{},\r\n      distance: \"1000\",\r\n      value: \"\",\r\n      columns5: [\r\n        {\r\n          title: \"公司名称\",\r\n          key: \"cname\",\r\n          align: \"center\",\r\n        },\r\n        {\r\n          title: \"岗位名称\",\r\n\t\t\t\t\twidth: 240,\r\n          key: \"jname\",\r\n          align: \"center\",\r\n        },\r\n        {\r\n          title: \"工作地区\",\r\n          key: \"workplace\",\r\n\t\t\t\t\talign: \"center\",\r\n        },\r\n\t\t\t\t{\r\n\t\t\t\t  title: \"薪资范围\",\r\n\t\t\t\t\tkey: \"salary\",\r\n\t\t\t\t\talign: \"center\",\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t  title: \"学历要求\",\r\n\t\t\t\t\tkey: \"edu\",\r\n\t\t\t\t\talign: \"center\",\r\n\t\t\t\t},\r\n        {\r\n      