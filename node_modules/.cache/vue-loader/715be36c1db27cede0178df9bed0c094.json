/div>\r\n        <div class=\"panel-line\"></div>\r\n      </div>\r\n    </div>\r\n    <div style=\"overflow: hidden;\">\r\n      <div style=\"float: right\">\r\n        <Input v-model=\"value\" placeholder=\"请输入房源所在小区名\" style=\"width: 200px;margin: 10px;\" />\r\n        <Button type=\"success\" @click=\"getOneinfo\" class=\"submit\">查询</Button>\r\n      </div>\r\n    </div>\r\n    <Table height=\"520\" stripe border :columns=\"columns5\" :data=\"data5\"></Table>\r\n  </Card>\r\n</template>\r\n<script>\r\nimport Server from \"@/core/server\";\r\nimport { services } from \"@/core/config/services\";\r\nimport houseInfoId from \"@/vuex/store\";\r\nimport dataRap from \"@/vuex/store\";\r\nimport aroundInfo from \"@/vuex/store\";\r\nimport housePoint from \"@/vuex/store\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      form:{},\r\n      columns5: [\r\n        {\r\n          title: \"房源图片\",\r\n          key: \"img\",\r\n          align: \"center\",\r\n          width: 180,\r\n          render: (h, params) => {\r\n            // console.log(params.row);\r\n            return h(\"img\", {\r\n              attrs: {\r\n                src: params.row.image,\r\n              },\r\n              style: {\r\n                marginRight: \"5px\",\r\n                height: \"80px\",\r\n                width: \"130px\",\r\n              },\r\n            });\r\n          },\r\n        },\r\n        {\r\n          title: \"小区名称\",\r\n          key: \"neighborhood\",\r\n          align: \"center\",\r\n        },\r\n        {\r\n          title: \"简介\",\r\n          key: \"title\",\r\n        },\r\n        {\r\n          title: \"地址\",\r\n          key: \"address\",\r\n        },\r\n        {\r\n          title: \"价格(元/月)\",\r\n          sortable: true,\r\n          key: \"price\",\r\n          align: \"center\",\r\n        },\r\n        {\r\n          title: \"详细信息\",\r\n          key: \"action\",\r\n          width: 150,\r\n          align: \"center\",\r\n          render: (h, params) => {\r\n            return h(\"div\", [\r\n              h(\r\n                \"Button\",\r\n                {\r\n                  props: {\r\n                    type: \"primary\",\r\n                    size: \"small\",\r\n                  },\r\n                  style: {\r\n                    marginRight: \"5px\",\r\n                  },\r\n                  on: {\r\n                    click: () => {\r\n                      this.getHouseInfo(params.row, params.index);\r\n                    },\r\n                  },\r\n                },\r\n                \"详情\"\r\n              ),\r\n            ]);\r\n          },\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  created() {\r\n    this.getinfo();\r\n  },\r\n  methods: {\r\n    getHouseInfo(currentRow, index) {\r\n      currentRow.Index = index;\r\n      this.selectedRow = currentRow;\r\n      var row = this.selectedRow;\r\n      this.houseName = row.neighborhood;\r\n      this.houseId = row.id;\r\n      housePoint.commit(\"housePoint\", currentRow.Shape);\r\n      Server.get({\r\n        url: services.compareHouseDetails,\r\n        params: {\r\n          houseId: this.houseId,\r\n        },\r\n      }).then((rsp) => {\r\n        if (rsp.code === 20000) {\r\n          houseInfoId.commit(\"houseInfoId\", rsp);\r\n          // console.log(houseInfoId)\r\n          aroundInfo.commit(\"aroundInfo\", rsp);\r\n          aroundInfo.commit(\"housueName\", this.houseName);\r\n          this.$router.push(\"/houseinfo\");\r\n        }\r\n      });\r\n    },\r\n    getinfo() {\r\n      var _this = this;\r\n      Server.get({\r\n        url: services.getallinfo,\r\n        params: {\r\n          name: _this.value,\r\n        },\r\n      }).then(function (res) {\r\n        _this.data5 = res.data.data;\r\n        // console.log(_this.data5);\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"less\" >\r\n// .ivu-layout {\r\n//     height: 1024px;\r\n// }\r\n.submit {\r\n  margin: 10px;\r\n  margin-right: 0;\r\n}\r\n.pageFoot {\r\n  margin-top: 10px;\r\n  float: right;\r\n}\r\n.group-panel-box {\r\n  &.blue {\r\n    .panel-title {\r\n      background: #47cb89;\r\n    }\r\n    .panel-line {\r\n      background: #47cb89;\r\n    }\r\n  }\r\n  .panel-title {\r\n    float: left;\r\n    padding: 0 10px;\r\n    height: 30px;\r\n    line-height: 30px;\r\n    color: white;\r\n    font-size: 14px;\r\n    margin-left: 10px;\r\n    clear: both;\r\n  }\r\n  .panel-line {\r\n    height: 2px;\r\n    clear: both;\r\n  }\r\n}\r\n.chartImage {\r\n  position: relative;\r\n  left: 0;\r\n  float: right;\r\n  top: -51px;\r\n  cursor: pointer;\r\n}\r\n.slelectInput {\r\n  width: 200px;\r\n  float: right;\r\n}\r\n\r\n.data-view {\r\n  padding: 20px;\r\n  height: 1450px;\r\n  .group-header {\r\n    height: 20px;\r\n  }\r\n  .ivu-row {\r\n    .left {\r\n      padding-right: 10px;\r\n    }\r\n    .right {\r\n      padding-left: 10px;\r\n    }\r\n    .chart {\r\n      height: 400px;\r\n      width: 100%;\r\n    }\r\n    .card-box {\r\n      .card-tools {\r\n        span {\r\n          float: left;\r\n        }\r\n        .ivu-select {\r\n          float: left;\r\n          margin: 4px 20px 4px 5px;\r\n          width: 150px;\r\n          .ivu-select-selection {\r\n            border-radius: 0;\r\n          }\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n.demo-spin-icon-load {\r\n  animation: ani-demo-spin 1s linear infinite;\r\n}\r\n</style>\r\n"]}]}