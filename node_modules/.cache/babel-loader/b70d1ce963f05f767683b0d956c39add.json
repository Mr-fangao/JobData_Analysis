"@/vuex/store\";\r\nimport aroundInfo from \"@/vuex/store\";\r\nimport housePoint from \"@/vuex/store\";\r\n\r\nexport default {\r\n  components: {\r\n    SiderMenu,\r\n    ChangePsd,\r\n  },\r\n  data() {\r\n    return {\r\n      BaseOnHouse: [],\r\n      personId: \"\",\r\n      userId: JSON.parse(sessionStorage.getItem(\"userId\")),\r\n      distance: \"1000\",\r\n      show: true,\r\n      modal1: false,\r\n      columns1: [\r\n        {\r\n          type: \"index\",\r\n          width: 60,\r\n          align: \"center\",\r\n          title: \"ID\",\r\n        },\r\n        {\r\n          align: \"center\",\r\n          title: \"公司名称\",\r\n          key: \"neighborhood\",\r\n        \twidth: 195,\r\n        },\r\n        {\r\n          align: \"center\",\r\n          title: \"职位名称\",\r\n          key: \"area\",\r\n        },\r\n        {\r\n          align: \"center\",\r\n          title: \"工作地区\",\r\n          key: \"floor\",\r\n        },\r\n        {\r\n          align: \"center\",\r\n          title: \"薪资区间\",\r\n          key: \"price\",\r\n        },\r\n        {\r\n          align: \"center\",\r\n          title: \"学历要求\",\r\n          key: \"faceDirection\",\r\n        },\r\n        {\r\n          align: \"center\",\r\n          title: \"工作经验\",\r\n          key: \"rentWay\",\r\n        },\r\n        {\r\n          title: \"操作\",\r\n          key: \"action\",\r\n          width: 150,\r\n          align: \"center\",\r\n          render: (h, params) => {\r\n            return h(\"div\", [\r\n              h(\r\n                \"Button\",\r\n                {\r\n                  props: {\r\n                    type: \"success\",\r\n                    size: \"small\",\r\n                  },\r\n                  style: {\r\n                    marginRight: \"5px\",\r\n                  },\r\n                  on: {\r\n                    click: () => {\r\n                      this.shows(params.row, params.index);\r\n                    },\r\n                  },\r\n                },\r\n                \"查看详情\"\r\n              ),\r\n              h(\r\n                \"Button\",\r\n                {\r\n                  props: {\r\n                    type: \"error\",\r\n                    size: \"small\",\r\n                  },\r\n                  on: {\r\n                    click: () => {\r\n                      this.remove(params.row, params.index);\r\n                    },\r\n                  },\r\n                },\r\n                \"删除\"\r\n              ),\r\n            ]);\r\n          },\r\n        },\r\n      ],\r\n      data1: [\r\n\t\t\t\t{\r\n\t\t\t\t\tneighborhood:'安徽美图信息科技有限公司',\r\n\t\t\t\t\tarea:'数据处理员',\r\n\t\t\t\t\tfloor:'合肥蜀山区',\r\n\t\t\t\t\tprice:'3-4千/月',\r\n\t\t\t\t\tfaceDirection:'大专',\r\n\t\t\t\t\trentWay:'1年',\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t\tneighborhood:'广州仓实信息科技有限公司',\r\n\t\t\t\t\tarea:'GIS软件开发工程师',\r\n\t\t\t\t\tfloor:'广州越秀区',\r\n\t\t\t\t\tprice:'0.7-1万/月',\r\n\t\t\t\t\tfaceDirection:'本科',\r\n\t\t\t\t\trentWay:'1-2年',\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t\tneighborhood:'湖北双可科技有限公司',\r\n\t\t\t\t\tarea:'GIS开发工程师',\r\n\t\t\t\t\tfloor:'武汉江夏区',\r\n\t\t\t\t\tprice:'0.8-1.4万/月',\r\n\t\t\t\t\tfaceDirection:'本科',\r\n\t\t\t\t\trentWay:'1-2年',\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t\tneighborhood:'武汉珞珈德毅科技股份有限公司',\r\n\t\t\t\t\tarea:'数据处理员',\r\n\t\t\t\t\tfloor:'武汉洪山区',\r\n\t\t\t\t\tprice:'1-2万/月',\r\n\t\t\t\t\tfaceDirection:'大专',\r\n\t\t\t\t\trentWay:'1-2年',\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t\tneighborhood:'天维尔信息科技股份有限公司',\r\n\t\t\t\t\tarea:'数据处理员',\r\n\t\t\t\t\tfloor:'深圳南山区',\r\n\t\t\t\t\tprice:'0.7-1.5万/月',\r\n\t\t\t\t\tfaceDirection:'本科',\r\n\t\t\t\t\trentWay:'1年',\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t\tneighborhood:'重庆华悦生态环境工程研究院有限公司',\r\n\t\t\t\t\tarea:'数据处理员',\r\n\t\t\t\t\tfloor:'深圳福田区',\r\n\t\t\t\t\tprice:'1-1.5万/月',\r\n\t\t\t\t\tfaceDirection:'本科',\r\n\t\t\t\t\trentWay:'2年',\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t\tneighborhood:'泰豪科技股份有限公司',\r\n\t\t\t\t\tarea:'数据处理员',\r\n\t\t\t\t\tfloor:'南昌高新开发区',\r\n\t\t\t\t\tprice:'1-1.5万/月',\r\n\t\t\t\t\tfaceDirection:'本科',\r\n\t\t\t\t\trentWay:'2年',\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t\tneighborhood:'南京益美沃电子科技有限公司',\r\n\t\t\t\t\tarea:'数据处理员',\r\n\t\t\t\t\tfloor:'南京雨花台区',\r\n\t\t\t\t\tprice:'0.8-1.2万/月',\r\n\t\t\t\t\tfaceDirection:'本科',\r\n\t\t\t\t\trentWay:'1年',\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t\tneighborhood:'杭州飞时达软件有限公司',\r\n\t\t\t\t\tarea:'数据处理员',\r\n\t\t\t\t\tfloor:'杭州西湖区',\r\n\t\t\t\t\tprice:'0.8-1.5万/月',\r\n\t\t\t\t\tfaceDirection:'大专',\r\n\t\t\t\t\trentWay:'1-2年',\r\n\t\t\t\t},\r\n\t\t\t\t{\r\n\t\t\t\t\tneighborhood:'苏州规划设计研究院股份有限公司昆山分公司',\r\n\t\t\t\t\tarea:'数据处理员',\r\n\t\t\t\t\tfloor:'苏州工业园区',\r\n\t\t\t\t\tprice:'0.9-2万/月',\r\n\t\t\t\t\tfaceDirection:'本科',\r\n\t\t\t\t\trentWay:'无需',\r\n\t\t\t\t},\r\n\t\t\t],\r\n      data2: [],\r\n      userInfo: [],\r\n      userage: [],\r\n      money: [],\r\n      educations: [],\r\n      houseShape: [],\r\n      houseId: [],\r\n    };\r\n  },\r\n  created() {\r\n    this.getTable();\r\n    this.getInfo();\r\n    this.getBaseOnPersonData();\r\n  },\r\n  methods: {\r\n    getBaseOnPersonData() {\r\n      var _this = this;\r\n      this.personId = this.userId;\r\n      \r\n      Server.get({\r\n        url: services.recommendedBaseOnPerson,\r\n        params: {\r\n          personId: _this.userId,\r\n        },\r\n      }).then(function (rsp) {\r\n        if (rsp.code === 20000) {\r\n          // console.log(rsp);\r\n          _this.BaseOnHouse = rsp.data.data;\r\n          // console.log(_this.BaseOnHouse);\r\n        }\r\n      });\r\n    },\r\n    getInfo() {\r\n      var this_ = this;\r\n      Server.get({\r\n        url: services.getInfo,\r\n        params: {\r\n          userId: this.userId,\r\n        },\r\n      }).then((rsp) => {\r\n        // console.log(rsp);\r\n        this.userInfo = rsp.data.data;\r\n      });\r\n    },\r\n    getTable() {\r\n      var this_ = this;\r\n      var userId = this_.userId;\r\n      Server.get({\r\n        url: services.getCollectHouseInfo,\r\n        params: {\r\n          userId: userId,\r\n        },\r\n      }).then((rsp) => {\r\n        console.log(rsp)\r\n        this_.data1 = rsp.data.data;\r\n      });\r\n    },\r\n    onCancel() {\r\n      this.modal1 = false;\r\n    },\r\n    onRefresh() {\r\n      this.getTable();\r\n      this.personalComments();\r\n    },\r\n    onSubmit() {\r\n      this.$refs.changepsd.onSubmit();\r\n    },\r\n    shows(currentRow, index) {\r\n      currentRow.Index = index;\r\n      this.selectedRow = currentRow;\r\n      var row = this.selectedRow;\r\n      this.houseName = row.name;\r\n      this.houseId = row.id;\r\n      console.log(this.houseId)\r\n      housePoint.commit(\"housePoint\", this.houseShape);\r\n      Server.get({\r\n        url: services.compareHouseDetails,\r\n        params: {\r\n          houseId: this.houseId,\r\n        },\r\n      }).then((rsp) => {\r\n            if (rsp.code === 20000) {\r\n              houseInfoId.commit(\"houseInfoId\", rsp)\r\n              aroundInfo.commit(\"aroundInfo\", rsp);\r\n              aroundInfo.commit(\"housueName\", this.houseName);\r\n              this.$router.push(\"/houseinfo\");\r\n            }\r\n          })\r\n    },\r\n    remove(currentRow, index) {\r\n      var userId = JSON.parse(sessionStorage.getItem(\"userId\"));\r\n      currentRow.Index = index;\r\n      this.selectedRow = currentRow;\r\n      var row = this.selectedRow;\r\n      if (!row) {\r\n        this.$Message.warning(\"请选择需要删除的行\");\r\n      } else {\r\n        this.$Modal.confirm({\r\n          title: \"提示\",\r\n          content: \"是否永久删除此数据?\",\r\n          onOk: () => {\r\n            // var id = row.houseId;\r\n            var name = row.name;\r\n            Server.get({\r\n              url: services.delCollectHouse,\r\n              params: {\r\n                userId: userId,\r\n                houseName: name,\r\n              },\r\n            }).then((rsp) => {\r\n              if (rsp.status == 1) {\r\n                this.$Message.success(rsp.message);\r\n                this.onRefresh();\r\n              } else {\r\n                this.$Message.error(rsp.message);\r\n              }\r\n            });\r\n          },\r\n          onCancel: () => {},\r\n        });\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"less\" scoped>\r\n.avatar {\r\n  text-align: center;\r\n}\r\n.ivu-tabs-bar {\r\n  margin-bottom: 0;\r\n}\r\n.layout {\r\n  // border: 1px solid #d7dde4;\r\n  background: #f5f7f9;\r\n  position: relative;\r\n  border-radius: 4px;\r\n  overflow: hidden;\r\n}\r\n\r\n.layout-header-bar {\r\n  background: #fff;\r\n  box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.layout-logo-left {\r\n  width: 90%;\r\n  height: 30px;\r\n  background: #5b6270;\r\n  border-radius: 3px;\r\n  margin: 15px auto;\r\n}\r\n\r\n.menu-icon {\r\n  transition: all 0.3s;\r\n}\r\n\r\n.rotate-icon {\r\n  transform: rotate(-90deg);\r\n}\r\n\r\n.menu-item span {\r\n  display: inline-block;\r\n  overflow: hidden;\r\n  width: 69px;\r\n  text-overflow: ellipsis;\r\n  white-space: nowrap;\r\n  vertical-align: bottom;\r\n  transition: width 0.2s ease 0.2s;\r\n}\r\n\r\n.menu-item i {\r\n  transform: translateX(0px);\r\n  transition: font-size 0.2s ease, transform 0.2s ease;\r\n  vertical-align: middle;\r\n  font-size: 16px;\r\n}\r\n\r\n.collapsed-menu span {\r\n  width: 0px;\r\n  transition: width 0.2s ease;\r\n}\r\n\r\n.collapsed-menu i {\r\n  transform: translateX(5px);\r\n  transition: font-size 0.2s ease 0.2s, transform 0.2s ease 0.2s;\r\n  vertical-align: middle;\r\n  font-size: 22px;\r\n}\r\n\r\n.ivu-form-item {\r\n  margin-bottom: 1px;\r\n}\r\n\r\n.ivu-divider-horizontal {\r\n  display: block;\r\n  height: 1px;\r\n  width: 100%;\r\n  min-width: 100%;\r\n  margin: 15px 0;\r\n  clear: both;\r\n}\r\n</style>\r\n"],"sourceRoot":"src\\views\\personalcenter\\component"}]}