rs\">\r\n            <thead>\r\n              <tr>\r\n                <th></th>\r\n                <th>距离</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody id=\"distance2\"></tbody>\r\n          </table>\r\n        </Modal>\r\n\r\n        <Modal v-model=\"modal3\" title=\"交通资源的中间过程数据\" :styles=\"{top: '55px'}\" width=\"700\">\r\n          <table id=\"customers\">\r\n            <thead>\r\n              <tr>\r\n                <th>小区名称</th>\r\n                <th>人口</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody id=\"population3\"></tbody>\r\n          </table>\r\n\r\n          <table id=\"customers\">\r\n            <thead>\r\n              <tr>\r\n                <th></th>\r\n                <th>服务能力</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody id=\"service3\"></tbody>\r\n          </table>\r\n\r\n          <table id=\"customers\">\r\n            <thead>\r\n              <tr>\r\n                <th></th>\r\n                <th>距离</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody id=\"distance3\"></tbody>\r\n          </table>\r\n        </Modal>\r\n        <Modal v-model=\"modal4\" title=\"休闲娱乐的中间过程数据\" :styles=\"{top: '55px'}\" width=\"700\">\r\n          <table id=\"customers\">\r\n            <thead>\r\n              <tr>\r\n                <th>小区名称</th>\r\n                <th>人口</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody id=\"population4\"></tbody>\r\n          </table>\r\n\r\n          <table id=\"customers\">\r\n            <thead>\r\n              <tr>\r\n                <th></th>\r\n                <th>服务能力</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody id=\"service4\"></tbody>\r\n          </table>\r\n\r\n          <table id=\"customers\">\r\n            <thead>\r\n              <tr>\r\n                <th></th>\r\n                <th>距离</th>\r\n              </tr>\r\n            </thead>\r\n     